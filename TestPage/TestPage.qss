}

QPushButton#m_pNextStepButton
{
    background: #248CEB;
    border-radius: 10px;
    border: 0px solid #529BFF;
    font-size:20px;
    font-family:Arial;
    color:#FFF;
    qproperty-iconSize:50px 50px;
    qproperty-icon: url(:/image/ico/main/fo.png) off, url(:/image/ico/main/go.png) on;
}
QPushButton#m_pStopTestButton
{
    background: #248CEB;
    border-radius: 10px;
    border: 0px solid #529BFF;
    font-size:20px;
    font-family:Arial;
    color:#FFF;
    qproperty-iconSize:50px 50px;
    qproperty-icon: url(:/image/ico/main/fo.png) off, url(:/image/ico/main/go.png) on;
}
QPushButton:pressed#m_pNextStepButton
{
    border: 0px solid #529BFF;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 #0EBEEF, stop: 1 #349DEE);
}


QPushButton:pressed#m_pStopTestButton
{
    border: 0px solid #529BFF;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 #0EBEEF, stop: 1 #349DEE);
}

