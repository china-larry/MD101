QComboBox#m_pAgeWidget
{
    height: 50px;
    min-width: 128px;
    max-width: 128px;
    color: #000; /* text color */
    font-family: "Arial";
    font-size: 22px;
    border: 2px solid #529BFF;
    border-radius:5px;

}
QComboBox
{
    height: 50px;
    min-width: 230px;
    max-width: 230px;
    color: #000; /* text color */
    font-family: "Arial";
    font-size: 22px;
    border: 2px solid #529BFF;
    border-radius:5px;

}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 40px;

    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
    image: url(:/image/ico/login/commod.png);
}
QComboBox QAbstractItemView
{
        border: 2px solid darkgray;/*下拉列表的边线的粗细、实虚*/
        selection-background-color: blue;/*下拉列表选中的行的背景色*/
        color: #000; /* text color */
        font-family: "Arial";
        font-size: 22px;
}
QComboBox QAbstractItemView::item
{
        min-height: 60px;/*下拉列表的行高，也可以看做行距*/
        color: #000; /* text color */
        font-family: "Arial";
        font-size: 22px;
}
/*垂直滚动条整体*/
QScrollBar:vertical
{
    width:25px;
    background:rgb(30, 141, 255,0%);
    margin:0px,0px,0px,0px;
    padding-top:25px;   /*上预留位置*/
    padding-bottom:25px;    /*下预留位置*/
}

